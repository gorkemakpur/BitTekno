@model BitTekno.data.Models.Product
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var images = BitTekno.Helper.GetProductWithImagesId(Model.ProductID);
    var productComment = BitTekno.Helper.GetProductWithProductCommentId(Model.ProductID);
    var productFeatures = BitTekno.Helper.GetProductWithProductFeatures(Model.ProductID);
    var color = BitTekno.Helper.GetProductWithProductColor(Model.ProductID);

   
}

<!-- breadcrumb area start -->
<div class="breadcrumb-area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb-wrap">
                    <nav aria-label="breadcrumb">
                        <ul class="breadcrumb">
                            <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Anasayfa</a></li>
                            <li class="breadcrumb-item"><a href="@Url.Action("Index","Department")">Kategori</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Ürün Detay</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- breadcrumb area end -->
<!-- product details wrapper start -->
<div class="product-details-wrapper">
    <div class="container">
        <div class="row">
            <div class="col-lg-9">
                <!-- product details inner end -->
                <div class="product-details-inner">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="product-large-slider mb-20 slick-arrow-style_2">

                                <div class="pro-large-img img-zoom" id="img1">
                                    <img src="~/wwwroot/img/product/Iphone11-2.jpg" alt="" />
                                </div>

                            </div>

                            <div class="pro-nav slick-padding2 slick-arrow-style_2">
                                @foreach (var item in images)
                                {
                                    <div class="pro-nav-thumb"><img src="~/wwwroot/img/product/@item.ImagesUrl" alt="" /></div>
                                }

                            </div>



                        </div>

                        <div class="col-lg-6">
                            <div class="product-details-des mt-md-34 mt-sm-34">
                                <h3><a href="product-details.html">@Model.ProductName</a></h3>
                                <div class="ratings">
                                    @for (int i = 0; i < productComment.Sum(x => x.Point) / productComment.Count; i++)
                                    {
                                        <span class="good"><i class="fa fa-star"></i></span>
                                    }

                                    <div class="pro-review">

                                        <span>@productComment.Count() Yorum Bulundu</span>
                                    </div>
                                </div>

                                <div class="availability mt-10">
                                    <h5>Stok Durumu:</h5>

                                    @if (Model.Stock > 0)
                                    {
                                        <span>
                                            Stokta @Model.Stock adet var
                                        </span>
                                    }
                                    else
                                    {
                                        <span style="color:red">
                                            Ürün stokta yok
                                        </span>
                                    }
                                </div>


                                <div class="pricebox">
                                    <span class="regular-price">₺@Model.PriceOutput</span>
                                </div>
                                <p>
                                    @Model.Description
                                </p>

                                <!--Sepete ekle kısmı-->
