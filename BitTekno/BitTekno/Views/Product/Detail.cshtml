                                        <div class="quantity">
                                            <div class="pro-qty"><input type="text" value="1"></div>
                                        </div>


                                        <div class="action_link">
                                            <i class="fa fa-shopping-cart"><input class="buy-btn" type="submit" value="Sepete Ekle" /></i>
                                        </div>

                                    </div>
                                </div>
                            </div>

                                                }
                                                }
                                                else
