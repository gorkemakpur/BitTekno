                                    @Html.HiddenFor(x => x.Reference2)
                                    @Html.HiddenFor(x => x.ColorID)
                                    @Html.HiddenFor(x => x.PriceOutput)
                                    @Html.HiddenFor(x => x.ProductBarcode)

